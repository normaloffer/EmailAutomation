<%- include('../include/header') %>
<section class="content-header">
    <h1>
        Dashboard
        <small>Mail Queue</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="/admin/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Mail Queue</li>
    </ol>
</section>
<div class="content">
    <div class="row">
        <div class="col-md-4">
            <div class="box box-primary">
                <div class="box-header">
                    <h3 class="box-title">Mail Queue</h3>
                </div>
                <div class="box-body">
                    <table class="table">
                        <tr>
                            <th>Incoming</th>
                            <td><%= result_data.incoming; %></td>
                        </tr>
                        <tr>
                            <th>Active</th>
                            <td><%= result_data.active; %></td>
                        </tr>
                        <tr>
                            <th>Deferred</th>
                            <td><%= result_data.deferred; %></td>
                        </tr>
                        <tr>
                            <th>Bounced</th>
                            <td><%= result_data.bounced; %></td>
                        </tr>
                        <tr>
                            <th>Hold</th>
                            <td><%= result_data.hold; %></td>
                        </tr>
                        <tr>
                            <th>Corrupt</th>
                            <td><%= result_data.corrupt; %></td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include('../include/footer') %>
<script>
    get_data_pagination();
</script>